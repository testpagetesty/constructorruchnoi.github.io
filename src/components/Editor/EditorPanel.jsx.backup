      /* Basic card styles with animations */
      .card {
        transition: all 0.3s ease-in-out;
        padding: 1.5rem;
        display: flex;
        flex-direction: column;
        background-color: #ffffff;
        width: 100%;
        max-width: 280px;
        border-radius: 12px;
        overflow: hidden;
        height: 100%;
        margin: 1rem;
        position: relative;
        z-index: 2;
